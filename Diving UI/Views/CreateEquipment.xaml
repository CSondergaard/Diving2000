<UserControl x:Class="Diving_UI.Views.CreateEquipment"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Diving_UI.Views"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid x:Name="window">
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label Content="Vælg kategori:" />
                <ComboBox x:Name="CBCategory" SelectionChanged="CBCategory_SelectionChanged" />
            </StackPanel>
            <StackPanel>
                <Label Content="Service:" />
                <xctk:DateTimePicker x:Name="dtpService" />
            </StackPanel> 
            <StackPanel x:Name="SpProp">

            </StackPanel>
            <Separator Width="100" Height="10" Background="Transparent" />
            <Button Content="Opret Equipment" Width="100" Height="25" x:Name="btnCreateEq" Click="btnCreateEq_Click" />
        </StackPanel>

    </Grid>
</UserControl>
